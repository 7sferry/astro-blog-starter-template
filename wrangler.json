{
  "main": "dist/_worker.js/index.js",
  "name": "ferry",
  "compatibility_date": "YYYY-MM-DD", // Update to the day you deploy
  "compatibility_flags": [
    "nodejs_compat",
    "global_fetch_strictly_public"
  ],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist"
  },
  "observability": {
    "enabled": true
  }
}
